SELECT \
  COUNT(CASE WHEN in_out = 'I' AND play_schedule = 'NT' THEN 1 END) AS normal_time_subs,\
  COUNT(CASE WHEN in_out = 'I' AND play_schedule = 'ST' THEN 1 END) AS stoppage_time_subs,\
  COUNT(CASE WHEN in_out = 'I' AND play_schedule = 'ET' THEN 1 END) AS extra_time_subs  \
FROM player_in_out;}
